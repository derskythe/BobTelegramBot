    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="CardId" type="xs:int" />
      <xs:element minOccurs="0" maxOccurs="1" name="OrderType" type="xs:string" />
      <xs:element minOccurs="1" maxOccurs="1" name="Period" type="xs:int" />
