      <xs:element minOccurs="1" maxOccurs="1" name="BirthDate" type="xs:dateTime" />
      <xs:element minOccurs="0" maxOccurs="1" name="RegAddress" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="LivingAddress" type="xs:string" />
