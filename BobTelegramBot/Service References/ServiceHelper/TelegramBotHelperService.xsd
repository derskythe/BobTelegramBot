    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" name="GetCardsAndBranchesResult" type="tns:BobSiteCardAndBranchListResult" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="BobSiteCardAndBranchListResult">
    <xs:complexContent mixed="false">
